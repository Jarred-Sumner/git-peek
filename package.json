{
  "name": "@jarred/git-peek",
  "version": "1.2.2",
  "author": "Jarred Sumner @Jarred-Sumner",
  "description": "Quickly preview remote Git repositories in your local editor",
  "bin": {
    "git-peek": "bin/git-peek"
  },
  "bugs": "https://github.com/Jarred-Sumner/git-peek/issues",
  "devDependencies": {
    "@types/js-yaml": "^4.0.0",
    "@types/node": "latest",
    "@types/rimraf": "^3.0.0",
    "@types/tar": "^4.0.4",
    "@vercel/fetch": "^6.1.0",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "edge-js": "^15.5.2",
    "esbuild": "^0.8.43",
    "git-url-parse": "^11.4.4",
    "ink": "^3.0.8",
    "ink-select-input": "^4.2.0",
    "ink-text-input": "^4.0.1",
    "keypress": "^0.2.1",
    "meow": "^9.0.0",
    "node-fetch": "^2.6.1",
    "normalize-git-url": "^3.0.2",
    "open-editor": "^3.0.0",
    "pkg": "^4.4.9",
    "promise-any-polyfill": "^0.1.2",
    "qs": "^6.9.6",
    "react": "^17.0.1",
    "release-it": "*",
    "rimraf": "^3.0.2",
    "simple-plist": "^1.1.1",
    "swr": "^0.4.2",
    "tar": "^6.1.0",
    "tmp": "^0.2.1",
    "typescript": "beta",
    "which": "^2.0.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "bin/git-peek"
  ],
  "homepage": "https://github.com/Jarred-Sumner/git-peek",
  "keywords": [
    "git-peek",
    "git",
    "github",
    "preview git",
    "editor"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "Jarred-Sumner/git-peek",
  "scripts": {
    "build": "node build.js",
    "test": "echo NO TESTS",
    "release": "dotenv release-it",
    "bt": "pnpm build && git-peek Jarred-Sumner/git-peek",
    "pkg-mac": "pkg --public --options='no-warnings' -t 'node14-macos-x64' ./bin/git-peek -o ./brew/git-peek-macOS",
    "pkg-windows": "pkg --public --options='no-warnings' -t 'node14-windows-x64' ./bin/git-peek -o ./brew/git-peek.exe",
    "pkg-all": "pnpm pkg-mac; pnpm pkg-windows",
    "prerelease": "pnpm pkg-all"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true,
      "assets": [
        "./brew/git-peek-macOS",
        "./brew/git-peek.exe"
      ]
    }
  }
}
